{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/patients/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "patient-service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/patients/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Key": "patients-catchall"
    },
    {
      "DownstreamPathTemplate": "/api/doctors/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "doctor-service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/doctors/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Key": "doctors-catchall"
    },
    {
      "DownstreamPathTemplate": "/api/billing/{catchAll}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "billing-service",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/billing/{catchAll}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "Key": "billing-catchall"
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}